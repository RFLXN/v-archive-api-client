{
  "name": "v-archive-api-client",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RFLXN/v-archive-api-client"
  },
  "exports": {
    "require": "./cjs/index.js",
    "import": "./esm/index.js"
  },
  "scripts": {
    "lint": "eslint --fix src/**/*.ts",
    "build:cjs": "tsc --p ./cjs/tsconfig.json",
    "build:esm": "tsc --p ./esm/tsconfig.json",
    "build": "yarpm run build:cjs && yarpm run build:esm",
    "prebuild": "yarpm run lint"
  },
  "devDependencies": {
    "@types/node": "^20.1.1",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "typescript": "^5.0.4",
    "yarpm": "^1.2.0"
  },
  "dependencies": {
    "axios": "^1.4.0"
  }
}
